<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <RelativeLayout style="@style/fragment_accomplish_relativeLayout">

            <TextView
                android:id="@+id/textView6"
                style="@style/fragment_accomplish_textview_right"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:text="李四" />

            <TextView
                style="@style/fragment_accomplish_textview_left"
                android:text="被考人姓名:" />
        </RelativeLayout>

        <RelativeLayout style="@style/fragment_accomplish_relativeLayout">

            <TextView
                android:id="@+id/textView8"
                style="@style/fragment_accomplish_textview_right"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:text="0" />

            <TextView
                style="@style/fragment_accomplish_textview_left"
                android:text="答题数:" />
        </RelativeLayout>

        <RelativeLayout style="@style/fragment_accomplish_relativeLayout">

            <TextView
                android:id="@+id/textView10"
                style="@style/fragment_accomplish_textview_right"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:text="0" />

            <TextView
                style="@style/fragment_accomplish_textview_left"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:text="优:" />
        </RelativeLayout>

        <RelativeLayout style="@style/fragment_accomplish_relativeLayout">

            <TextView
                android:id="@+id/textView12"
                style="@style/fragment_accomplish_textview_right"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:text="0" />

            <TextView
                style="@style/fragment_accomplish_textview_left"
                android:text="良:" />
        </RelativeLayout>

        <RelativeLayout style="@style/fragment_accomplish_relativeLayout">

            <TextView
                android:id="@+id/textView14"
                style="@style/fragment_accomplish_textview_right"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:text="0" />

            <TextView
                style="@style/fragment_accomplish_textview_left"
                android:text="差:" />
        </RelativeLayout>

        <RelativeLayout style="@style/fragment_accomplish_relativeLayout">

            <TextView
                android:id="@+id/textView16"
                style="@style/fragment_accomplish_textview_right"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:text="0" />

            <TextView
                style="@style/fragment_accomplish_textview_left"
                android:text="未回答:" />
        </RelativeLayout>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@android:color/white"
        android:padding="10dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/btn_no"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="18dp"
            android:text="是否通过,请评判:"
            android:textSize="15sp" />

        <Button
            android:id="@+id/btn_yes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:text="通过" />

        <Button
            android:id="@+id/btn_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/btn_yes"
            android:layout_alignBottom="@+id/btn_yes"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:text="未通过" />
    </RelativeLayout>

    <WebView
        android:id="@+id/wwv"
        android:layout_width="0dp"
        android:layout_height="0dp" />
</LinearLayout>